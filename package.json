    "@react-navigation/stack": "6.1.1",
    "@reduxjs/toolkit": "^1.6.2",
    "dayjs": "^1.10.6",
    "expo": "^44.0.0",
    "expo-analytics": "^1.0.18",
    "expo-analytics-amplitude": "~11.1.0",
    "expo-asset": "~8.4.6",
    "expo-av": "~10.2.0",
    "expo-blur": "~11.0.0",
    "expo-constants": "~13.0.1",
