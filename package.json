    "@types/jest": "^26.0.24",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-calendars": "^1.1264.2",
    "@types/uuid": "^8.3.1",
    "cypress": "^8.7.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
